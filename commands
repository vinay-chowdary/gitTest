git --version
git config --global user.name "vinay-chowdary"
git config --global user.email "vinaychowdary.dev@gmail.com"
git init
git add filename
git status
git rm --cached -r .    : to remove staged files
git log
git log --oneline
git diff <file>
git checkout <commit> <filename>
git reset HEAD <filename> : reset HEAD to previous commit
git checkout -- <filename> : discards all changes after the previous commit
git restore <file>         : to discard changes in working directory
git remote add origin "url"
git push origin master -u
git clone "url" <optional:filename>
git commit -a --allow-empty-message -m ""
git remote -v : verify remote origin
git remote set-url origin "url" : to change remote origin
git checkout <branch name>
git merge <branch name>
git branch : displays all branches
git branch <branchname> : create branch